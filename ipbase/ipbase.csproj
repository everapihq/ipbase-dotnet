<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>Ipbase</PackageId>
    <Version>1.0.1</Version>
    <Authors>Benedikt Stoy</Authors>
    <Company>Everapi GmbH</Company>
    <PackageTags>Ip;Ipbase;Api;Ipcheck;IP address converter; IPv4;IPv6</PackageTags>
    <Title>Ipbase Api</Title>
    <Description>
      This client library enables working with the Ipbase API - https://ipbase.com/
      Register here for an Apikey to use the API - https://app.currencyapi.com/register
      See Github repository here - https://github.com/everapihq/ipbase-dotnet
    </Description>
    <Summary>This is the official NuGet package for ipbase API.(https://ipbase.com/)</Summary>
    <Copyright>Copyright Â© Everapi GmbH 2022</Copyright>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageIcon>ipbase-icon.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageProjectUrl>https://ipbase.com/</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PublishRepositoryUrl>true</PublishRepositoryUrl> 
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Nullable>enable</Nullable>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="**\*.orig" />
    <None Include="..\LICENSE.md" Pack="true" PackagePath="LICENSE.md" />
    <None Include="..\ipbase.png" Pack="true" PackagePath="ipbase.png" />
    <None Include="..\ipbase-icon.png" Pack="true" PackagePath="ipbase-icon.png" />
    <None Include="..\README.md" Pack="true" PackagePath="README.md" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Helpers\" />
    <None Remove="Newtonsoft.Json" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Helpers\" />
  </ItemGroup>
</Project>
